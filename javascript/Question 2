function rateLimit(fn, delay) {
    let isCooldown = false;

    return function (...args) {
        if (isCooldown) return;

        fn.apply(this, args);

        isCooldown = true;

        setTimeout(() => {
            isCooldown = false;
        }, delay);
    };
}



const limitedFn = rateLimit(() => {
    console.log("Executed at", new Date().toLocaleTimeString());
}, 2000);

limitedFn(); 
limitedFn(); 
